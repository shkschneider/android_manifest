  <remove-project name="platform/packages/apps/Messaging" />
  <project path="packages/apps/Messaging" name="shkschneider/android_packages_apps_Messaging" remote="github" revision="shk-marshmallow" />

  <remove-project name="platform/packages/apps/Settings" />
  <project path="packages/apps/Settings" name="shkschneider/android_packages_apps_Settings" remote="github" revision="shk-marshmallow" />

  <project path="vendor/shk" name="shkschneider/android_vendor_shk" remote="github" revision="shk-marshmallow" />

